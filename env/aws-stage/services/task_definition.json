[
  {
    "name": "scheduled-tasks-services",
    "image": "benjohns1/scheduled-tasks-services",
    "portMappings": [
      {
        "containerPort": 3102,
        "hostPort": 3102,
        "protocol": "tcp"
      }
    ],
    "essential": true,
    "environment": [
      {
        "name": "APPLICATION_PORT",
        "value": "3102"
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "${logname}",
        "awslogs-region": "${logregion}",
        "awslogs-stream-prefix": "ecs"
      }
    }
  }
]
